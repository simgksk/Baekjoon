#include <iostream>

using namespace std;

	int main()
	{
#pragma region 9713. Sum of Odd Sequence

		int t, n;

		cin >> t;
		
		for (int i = 0; i < t; i++)
		{
			int sum = 0;

			cin >> n;

			for (int j = 1; j <= n; j++)
			{
				if (j % 2 == 1)
					sum += j;
			}

			cout << sum << endl;
		}

#pragma endregion

		return 0;
	}
