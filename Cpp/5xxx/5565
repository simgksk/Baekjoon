#include <iostream>

using namespace std;

	int main()
	{
#pragma region 5565. 영수증

		int priceNo, price;
		int addPrice = 0;

		cin >> priceNo;

		for (int i = 0; i < 9; i++)
		{
			cin >> price;
			addPrice += price;
		}

		cout << priceNo - addPrice << endl;

#pragma endregion

		return 0;
	}
